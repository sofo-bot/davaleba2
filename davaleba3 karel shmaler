//put Beeper on every second street
function main() {
  while(frontIsClear()) {
      move and put Beeper();
   }
}

function move and put beeper() {
   turnLeft();
   move();
   move();
   turnRight();
   move();
   turnRight();
   putBeeper();
   move();
   putBeeper();
   move();
   putBeeper();
   turnLeft(); 
   if (frontIsClear()) {
   move();
}
}
